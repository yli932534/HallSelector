<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/search_layout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#F4BFD1"
    tools:context=".SearchActivity">

    <CheckBox
        android:id="@+id/checkBox_vending"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="48dp"
        android:fontFamily="@font/a"
        android:onClick="onCheckboxClicked"
        android:text="@string/check_box_vending"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/new_search"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.024" />

    <CheckBox
        android:id="@+id/checkBox_microwave"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/a"
        android:onClick="onCheckboxClicked"
        android:text="@string/check_box_microwave_n_refrigerator"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.159"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.129" />

    <CheckBox
        android:id="@+id/checkBox_ac"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/a"
        android:onClick="onCheckboxClicked"
        android:text="@string/check_box_ac"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.148"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.393" />

    <CheckBox
        android:id="@+id/checkBox_kitchen"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/a"
        android:onClick="onCheckboxClicked"
        android:text="@string/check_box_kitchen"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.151"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.261" />

    <CheckBox
        android:id="@+id/checkBox_roombath"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="48dp"
        android:layout_marginTop="4dp"
        android:fontFamily="@font/a"
        android:onClick="onCheckboxClicked"
        android:text="Suite/Room Bath(missing in @str)"
        app:layout_constraintStart_toEndOf="@+id/new_search"
        app:layout_constraintTop_toBottomOf="@+id/checkBox_laundry" />

    <CheckBox
        android:id="@+id/checkBox_studyarea"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/a"
        android:onClick="onCheckboxClicked"
        android:text="@string/check_box_studyarea"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.143"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.526" />

    <CheckBox
        android:id="@+id/checkBox_laundry"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="48dp"
        android:layout_marginTop="8dp"
        android:fontFamily="@font/a"
        android:onClick="onCheckboxClicked"
        android:text="@string/check_box_laundry"
        app:layout_constraintStart_toEndOf="@+id/new_search"
        app:layout_constraintTop_toBottomOf="@+id/checkBox_handicap" />

    <CheckBox
        android:id="@+id/checkBox_academic"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="48dp"
        android:layout_marginTop="8dp"
        android:fontFamily="@font/a"
        android:onClick="onCheckboxClicked"
        android:text="@string/check_box_academic_year_building"
        app:layout_constraintStart_toEndOf="@+id/new_search"
        app:layout_constraintTop_toBottomOf="@+id/checkBox_roombath" />

    <CheckBox
        android:id="@+id/checkBox_gameroom"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="48dp"
        android:layout_marginTop="8dp"
        android:fontFamily="@font/a"
        android:onClick="onCheckboxClicked"
        android:text="@string/check_box_game_room"
        app:layout_constraintStart_toEndOf="@+id/new_search"
        app:layout_constraintTop_toBottomOf="@+id/checkBox_academic" />

    <CheckBox
        android:id="@+id/checkBox_lounge"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/a"
        android:onClick="onCheckboxClicked"
        android:text="@string/check_box_lounge_space"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.145"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.782" />

    <CheckBox
        android:id="@+id/checkBox_trashremoval"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/a"
        android:onClick="onCheckboxClicked"
        android:text="@string/check_box_own_trash_removal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.154"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.65" />

    <CheckBox
        android:id="@+id/checkBox_handicap"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="48dp"
        android:layout_marginTop="4dp"
        android:fontFamily="@font/a"
        android:onClick="onCheckboxClicked"
        android:text="@string/check_box_handicap"
        app:layout_constraintStart_toEndOf="@+id/new_search"
        app:layout_constraintTop_toBottomOf="@+id/checkBox_vending" />

    <Button
        android:id="@+id/new_search"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/a"
        android:onClick="onClickDone"
        android:text="@string/done"
        app:backgroundTint="#FF3535"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.514"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.976" />

    <Spinner
        android:id="@+id/area_spinner"
        android:layout_width="141dp"
        android:layout_height="38dp"
        android:fontFamily="@font/a"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.155"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.023" />

</androidx.constraintlayout.widget.ConstraintLayout>